#!/usr/bin/env bash

set -eu

source ${EAVE_HOME}/develop/shared/functions.bash
source ${EAVE_HOME}/develop/python/functions.bash
python-validate-version
python-activate-venv

if test -f requirements-vendor.txt
then
	rm -rf vendor
	python -m pip install -t vendor -r requirements-vendor.txt
fi

${EAVE_HOME}/develop/shared/bin/deploy-appengine
